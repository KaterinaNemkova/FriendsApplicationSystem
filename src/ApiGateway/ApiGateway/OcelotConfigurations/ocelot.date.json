{
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5100"
  },
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/dates",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "eventsservice_api",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/dates",
      "UpstreamHttpMethod": [
        "POST", "GET"
      ],
      "SwaggerKey": "Events"
    },

    {
      "DownstreamPathTemplate": "/api/dates/{dateId}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "eventsservice_api",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/dates/{dateId}",
      "UpstreamHttpMethod": [
        "DELETE"
      ],

      "SwaggerKey": "Events"
    },

    {
      "DownstreamPathTemplate": "/api/dates/{dateId}/changes",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "eventsservice_api",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/dates/{dateId}/changes",
      "UpstreamHttpMethod": [
        "PUT"
      ],
      "SwaggerKey": "Events"
    },
    

    {
      "DownstreamPathTemplate": "/api/dates/my-dates/{profileId}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "eventsservice_api",
          "Port": 8080
        }
      ],
      "UpstreamPathTemplate": "/dates/my-dates/{profileId}",
      "UpstreamHttpMethod": [
        "GET"
      ],
      "SwaggerKey": "Events"
    }
  ]
}