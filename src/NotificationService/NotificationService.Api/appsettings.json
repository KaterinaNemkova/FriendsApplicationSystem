{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "MongoDb": "__MONGO_DB3_CONNECTION_STRING__"
  },
  "TelegramBot": {
    "AccessToken": "__TELEGRAM_BOT__"
  },
  "AuthGrpcUrl" : {
    "GrpcUrl" : "__AUTH_GRPC_URL__"
  },
  "UserGrpcUrl":{
    "GrpcUrl": "__USER_GRPC_URL__"
  },
  "MessageBroker": {
    "HostName": "__RABBITMQ_HOSTNAME__",
    "Username": "__RABBITMQ_DEFAULT_USER__",
    "Password": "__RABBITMQ_DEFAULT_PASS__",
    "Port": "__RABBITMQ_PORT__",
    "Queues": {
      "FriendRequest": "FriendRequestsQueue",
      "ProfileCreated": "ProfileCreatedQueue",
      "MeetingRequest": "MeetingRequestsQueue",
      "DateRequest": "DateRequestsQueue",
      "GoalRequest": "GoalRequestsQueue"
    }
  }
          
}
